{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning"
    },
    "File": {
      "Path": "../logs"
  }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "HttpsDefaultCert": {
        "Url": "https://localhost:5000"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "localhost.pem",
        "KeyPath": "localhost-key.pem"
      }
    }
  }
}
